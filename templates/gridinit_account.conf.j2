# {{ ansible_managed }}
[Service.{{ openio_account_namespace }}-account-{{ openio_account_serviceid }}]
command=/usr/bin/oio-account-server /etc/oio/sds/{{ openio_account_namespace }}/account-{{ openio_account_serviceid }}/account-{{ openio_account_serviceid }}.conf
enabled=true
start_at_boot=true
on_die=respawn
group={{ openio_account_namespace }},account,{{ openio_account_serviceid }}
uid=openio
gid=openio
env.PATH=/usr/local/bin:/usr/bin:/usr/local/sbin:/usr/sbin
